{
  "containerDefinitions": [
      {
          "environment": [
              {
                  "name": "POSTGRES_DB",
                  "value": "postgres"
              },
              {
                  "name": "POSTGRES_USER",
                  "value": "postgres"
              },
              {
                  "name": "POSTGRES_PASSWORD",
                  "value": "postgres"
              }
          ],
          "essential": true,
          "image": "postgres",
          "name": "db"
      },
      {
          "command": [
              "python",
              "manage.py",
              "runserver",
              "0.0.0.0:8000"
          ],
          "essential": true,
          "mountPoints": [
              {
                  "containerPath": "/code",
                  "sourceVolume": "_"
              }
          ],
          "name": "web",
          "portMappings": [
              {
                  "containerPort": 8000,
                  "hostPort": 8000
              }
          ]
      }
  ],
  "family": "",
  "volumes": [
      {
          "host": {
              "sourcePath": "."
          },
          "name": "_"
      }
  ]
}
